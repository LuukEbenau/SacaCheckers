<UserControl x:Class="Dammen.UC.UCArrow" x:Name="ucarrow"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
             xmlns:local="clr-namespace:Dammen.UC"
             mc:Ignorable="d"
             d:DesignHeight="450" d:DesignWidth="800">
    <Grid>
        <Line X1="{Binding X1,ElementName=ucarrow}" Y1="{Binding Y1,ElementName=ucarrow}"
              X2="{Binding X2,ElementName=ucarrow}" Y2="{Binding Y2,ElementName=ucarrow}"
              Stroke="{Binding Stroke,ElementName=ucarrow}" />
        <!--<Line X2="{Binding X2,ElementName=ucarrow}" Y2="{Binding Y2,ElementName=ucarrow}" />
        <Line X2="{Binding X2,ElementName=ucarrow}" Y2="{Binding Y2,ElementName=ucarrow}" />-->
    </Grid>
</UserControl>